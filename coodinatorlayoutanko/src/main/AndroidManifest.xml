<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:dist="http://schemas.android.com/apk/distribution"
          package="com.pedroabinajm.coodinatorlayoutanko">
    <dist:module
            dist:onDemand="false"
            dist:title="@string/title_coodinatorlayoutanko">
        <dist:fusing dist:include="true"/>
    </dist:module>

    <application>
        <activity android:name=".CoordinatorLayoutAnkoActivity"
                  android:theme="@style/AppTheme.NoToolbar">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <data android:scheme="coordinatorlayout-anko"
                      android:host="anko-showroom"/>
            </intent-filter>
        </activity>
    </application>

</manifest>

